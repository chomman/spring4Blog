<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="query">
    <select id="getTest" resultType="java.lang.String">
        SELECT AME_Email
          FROM TB_JAM_AMember
         WHERE AME_SEQ = 4
    </select>

    <select id="getList" resultType="Member" parameterType="Member">
      SELECT AME_SEQ
            ,AME_MemID
            ,AME_Name
            ,AME_Email
            ,AME_Tel
            ,AME_Phone
            ,AME_Position
        FROM TB_JAM_AMember
        <where>
            <if test="AME_SEQ != null">
                AME_SEQ = #{AME_SEQ}
            </if>
        </where>
    </select>
</mapper>